

@font-face {
  font-family: 'GT Walsheim';
  font-display: swap;
  src: local('GT Walsheim Light'), local('GTWalsheimLight'),
    url('~assets/fonts/GTWalsheimLight.woff2') format('woff2'),
    url('~assets/fonts/GTWalsheimLight.woff') format('woff');
  font-weight: 300;
  font-style: normal;
  font-stretch: normal;
  unicode-range: U+0020-00FE;
}
@font-face {
  font-family: 'GT Walsheim';
  font-display: swap;
  src: local('GT Walsheim Regular'), local('GTWalsheimRegular'),
    url('~assets/fonts/GTWalsheimRegular.woff2') format('woff2'),
    url('~assets/fonts/GTWalsheimRegular.woff') format('woff');
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  unicode-range: U+0020-00FE;
}
@font-face {
  font-family: 'Schnyder L';
  font-display: swap;
  src: local('Schnyder L Demi Italic'), local('Schnyder L Demi Italic'),
    url('~assets/fonts/SchnyderL-DemiItalic.woff2') format('woff2'),
    url('~assets/fonts/SchnyderL-DemiItalic.woff') format('woff');
  font-weight: 600;
  font-style: italic;
  font-stretch: normal;
  unicode-range: U+0020-00FE;
}
@font-face {
  font-family: 'Schnyder L';
  font-display: swap;
  src: local('Schnyder L Demi'), local('SchnyderL-Demi'),
    url('~assets/fonts/SchnyderL-Demi.woff2') format('woff2'),
    url('~assets/fonts/SchnyderL-Demi.woff') format('woff');
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  unicode-range: U+0020-00FE;
}

